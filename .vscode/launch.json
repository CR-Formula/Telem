{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Cortex Debug",
            "cwd": "${workspaceFolder}",
            "preLaunchCommands": ["make -j8"],
            "executable": "./build/Telem.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "stutil",
            "liveWatch": {
                "enabled": true,
                "samplesPerSecond": 4
            },
            "device": "STM32F415",
            "interface": "swd",
            "svdFile": "${workspaceFolder}/STM32F405.svd",
        }
    ]
}